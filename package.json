{
  "name": "supercards",
  "version": "1.0.0",
  "description": "word cards app",
  "main": "supercards.js",
  "scripts": {
    "heroku-postbuild": "npm run build",
    "testd": "ts-node ./test.ts",
    "cleanup": "ts-node ./database/cleanup.ts",
    "test": "ts-mocha ./test/ui.test.ts",
    "build": "set ENV=production&&webpack",
    "build-dev": "set ENV=development&&webpack",
    "webpack-watch": "webpack-dev-server --hot --inline --host 0.0.0.0 --content-base ./public",
    "server-dev": "set NODE_ENV=development&&nodemon",
    "server-prod": "set NODE_ENV=production&&ts-node ./server/main.ts",
    "start": "concurrent --kill-others \"npm run webpack-watch\" \"npm run server-dev\""
  },
  "author": "phoenix",
  "license": "GPL-3.0",
  "dependencies": {
    "@types/express": "^4.0.36",
    "@types/jsdom": "^11.0.1",
    "@types/mocha": "^2.2.41",
    "@types/mysql": "0.0.34",
    "@types/node": "^8.0.28",
    "@types/react": "^15.6.0",
    "@types/react-dom": "^15.5.1",
    "@types/react-redux": "^4.4.47",
    "@types/react-router": "^3.0.1",
    "@types/redux-logger": "^3.0.3",
    "@types/selenium-webdriver": "^3.0.4",
    "@types/superagent": "^3.5.3",
    "@types/winston": "^2.3.5",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "body-parser": "^1.15.0",
    "connect": "^2.30.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "express-mysql-session": "^1.2.1",
    "express-session": "^1.15.3",
    "history": "^4.5.1",
    "jsdom": "^10.1.0",
    "mysql": "^2.13.0",
    "react": "^15.6.0",
    "react-dom": "^15.5.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "2.2.0",
    "superagent": "^3.5.0",
    "ts-loader": "^2.3.2",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.2",
    "webpack": "^3.4.1"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "chai-string": "^1.4.0",
    "chromedriver": "^2.29.0",
    "concurrently": "^3.1.0",
    "mocha": "^3.5.0",
    "selenium-webdriver": "^3.4.0",
    "ts-mocha": "^1.0.3",
    "webpack-dev-server": "^2.6.1"
  }
}
